<Window x:Class="app_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:app_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="625">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="42"/>
            <RowDefinition/>
            <RowDefinition Height="42"/>
        </Grid.RowDefinitions>
        <Rectangle HorizontalAlignment="Center" Height="42" Grid.Row="2" Stroke="Black" VerticalAlignment="Center" Width="625" StrokeThickness="0"/>
        <Button x:Name="ButtonBack" Content="Назад" HorizontalAlignment="Left" Margin="10,6,0,0" VerticalAlignment="Top" Width="75" Click="ButtonBack_OnClick"/>
        <Frame  x:Name="MainFrame" Content="MainFrame" Grid.Row="1" HorizontalAlignment="Center" Width="625" Height="350" VerticalAlignment="Bottom" Navigated="Frame_Navigated_1" Source="/Pages/AuthPage .xaml"/>

    </Grid>
</Window>
